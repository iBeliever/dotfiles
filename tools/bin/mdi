#! /usr/bin/env python3

import requests
import sys
import re
import codecs

URL = "http://cdn.materialdesignicons.com/2.0.46/css/materialdesignicons.min.css"

r = requests.get(URL)
css = r.text

filename = sys.argv[1]
pattern = r'\.mdi-' + re.escape(filename) + r':before{content:"\\([^"]+)"}'

match = re.search(pattern, css)
icon_code = int(match.group(1), base=16)
icon = chr(icon_code)

print(icon)
